{
  "identifier": "plugins",
  "description": "Permissions for plugin management functionality",
  "windows": ["main"],
  "permissions": [
    "core:app:default",
    "core:event:default",
    "core:window:default",
    "core:webview:default",
    "core:menu:default",
    "core:path:allow-resolve-directory",
    "core:path:allow-resolve",
    "core:path:allow-normalize",
    "core:path:allow-join",
    "core:path:allow-basename"
  ]
}